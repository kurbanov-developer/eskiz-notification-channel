{
    "name": "kurbanov-developer/eskiz-notification-channel",
    "description": "Eskiz SMS Notification Channel for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ismoil Kurbonov",
            "email": "kurbanov.developer@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.2",
        "phpunit/phpunit": "^9.0|^10.4"
    },
    "autoload": {
        "psr-4": {
            "KurbanovDeveloper\\EskizNotificationChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KurbanovDeveloper\\EskizNotificationChannel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KurbanovDeveloper\\EskizNotificationChannel\\EskizServiceProvider"
            ]
        }
    }
}
